
#grade converter
def grade_converter(grade):
    if grade >= 90:
        return "A"
    elif 89 >= grade >= 80:
        return "B"
    elif 79 >= grade >= 70:
        return "C"
    elif 69 >= grade >= 65:
        return "D"
    else:
        return "F"

#TESTS
#this should print "A"
print(grade_converter(95))

#this should print "F"
print(grade_converter(40))

#this should print "C"
print(grade_converter(73))

