def c_to_f(c_temp):
    tempf = c_temp * (9/5) + 32
    return tempf

#enter (c) value where 0 is.
tc = c_to_f(0)
print("The temperature in farenheit is " + str(tc))

def f_to_c(f_temp):
    tempc = (f_temp - 32) * 5/9 
    return tempc

#enter (f) value to where 32 is.
tf = f_to_c(32)
print("The temperature in celsius is " + str(tf))
